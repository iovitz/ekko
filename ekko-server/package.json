{
  "name": "nest-server",
  "version": "0.0.1",
  "private": true,
  "description": "A NestJS Project.",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nest start --watch",
    "start": "nest start",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "pnpx @compodoc/compodoc -p tsconfig.json -s -o -c .compodoc.json",
    "yac": "node_modules/.bin/httpyac -e dev -o body ./http.rest"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/platform-socket.io": "^10.4.6",
    "@nestjs/swagger": "^8.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.6",
    "@prisma/client": "^5.21.1",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.5.0",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "ejs": "^3.1.10",
    "express-session": "^1.18.1",
    "graphql": "^16.9.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nanoid": "^3.3.7",
    "pako": "^2.1.0",
    "prisma": "^5.21.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "statuses": "^2.0.1",
    "svg-captcha": "^1.4.0",
    "triple-beam": "^1.4.1",
    "typeorm": "^0.3.20",
    "ua-parser-js": "^1.0.39",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.14",
    "@types/json-stringify-safe": "^5.0.3",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.2",
    "@types/pako": "^2.0.3",
    "@types/statuses": "^2.0.5",
    "@types/supertest": "^6.0.2",
    "@types/triple-beam": "^1.3.5",
    "@types/ua-parser-js": "^0.7.39",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "httpyac": "^6.16.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
