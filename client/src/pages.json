{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "T O U C H"
			}
		},
		{
			"path": "pages/entry/entry",
			"style": {
				"navigationBarTitleText": "T O U C H"
			}
		},
		{
			"path": "pages/publish/publish",
			"style": {
				"navigationBarTitleText": "发 布 想 法"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "T O U C H"
			}
		},
		{
			"path": "pages/diary/diary",
			"style": {
				"navigationBarTitleText": "详情"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#414a62",
		"backgroundColor": "#414a62",
		"app-plus": {
      // APP端不展示顶部标题栏
		  "titleNView": false,
      // 关闭App端过度滚动回弹
      "bounce":"none"
		}
	}
}
